{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/essential", "@vue/airbnb"],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "quotes": "off",
    "comma-dangle": "off",
    "global-require": "off",
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "arrow-parens": "off",
    "arrow-body-style": "off"
  },
  "plugins": ["graphql"]
}
